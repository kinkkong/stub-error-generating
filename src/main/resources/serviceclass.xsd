<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"  xmlns:tns="http://haos.io/gant/service"
           targetNamespace="http://haos.io/gant/service" elementFormDefault="qualified"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
           xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
           jaxb:version="2.0"
           jaxb:extensionBindingPrefixes="xjc">

    <!--<xs:annotation>-->
